@model IEnumerable<BA_ERPMVC.ViewModels.OrderBooking.ReadyForDispatchedViewModel>

@{
    //if (Session["UserName"] == null)
    //{
    //    Response.Redirect("~/Account/Login");
    //}
    ViewBag.Title = "Expenses";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div id="gridShow" class="card">
    <div class="card-header" style="background:white;">
        <div class="row" style="margin-bottom:10px">
            <div class="col-sm-6 float-sm-left">
                <h4> Ready For Dispatched</h4>
            </div><!-- /.col -->

        </div>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 table-responsive">
                <table id="example" class="table table-bordered table-sm border-right-0 border-left-0 fs-7 table-v-middle table-responsive datatable" style="height:auto;">
                    <thead class="thead-light text-center">
                        <tr>
                            <th>Booking No#</th>
                            <th>Container No#</th>
                            <th>Container Size</th>
                            <th>DO Guarantee Document Date</th>
                            <th>Import EIR Document Date</th>
                            <th>Port Weight Document Date</th>
                            <th>Weighment OutSide Port Document Date</th>
                            <th>GD Document Date</th>
                            <th>BL Document Date</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var readyForDispatched in Model.ToList())
                        {
                            <tr class="bg-light tbl-valign-top">
                                <td>
                                    <input type="hidden" value="@readyForDispatched.OrderId" class="form-control txt_OrderId" />
                                    <input type="hidden" value="@readyForDispatched.BLnumber" class="form-control txt_BLnumber" />
                                    <input type="hidden" value="@Convert.ToInt32(readyForDispatched.ID)" class="form-control txt_ID" />
                                    <input type="text" disabled style="width: 118px;" value="@readyForDispatched.OrderNo" class="form-control txt_OrderNo" />
                                </td>
                                <td>
                                    <input type="text" disabled style="width: 150px;" value="@readyForDispatched.ContainerNo" class="form-control txt_ContainerNo" />
                                </td>
                                <td>
                                    <input type="text" disabled value="@readyForDispatched.ContainerSize" class="form-control txt_ContainerSize" />
                                </td>
                                <td>
                                    <input class="date-picker form-control txt_DOGranty" style="width: 170px;" type="date" placeholder="" value="@(readyForDispatched.DOGranty.HasValue ? readyForDispatched.DOGranty.Value.ToString("yyyy-MM-dd"): "")" />
                                </td>
                                <td>
                                    <input class="date-picker form-control txt_ImportEIR" style="width: 170px;" type="date" placeholder="" value="@(readyForDispatched.ImportEIR.HasValue ? readyForDispatched.ImportEIR.Value.ToString("yyyy-MM-dd"): "")" />
                                </td>
                                <td>
                                    <input class="date-picker form-control txt_PortWeighment" style="width: 170px;" type="date" placeholder="" value="@(readyForDispatched.PortWeighment.HasValue ? readyForDispatched.PortWeighment.Value.ToString("yyyy-MM-dd"): "")" />
                                </td>
                                <td>
                                    <input class="date-picker form-control txt_OutSidePortWeighment" style="width: 170px;" type="date" placeholder="" value="@(readyForDispatched.OutSidePortWeighment.HasValue ? readyForDispatched.OutSidePortWeighment.Value.ToString("yyyy-MM-dd"): "")" />
                                </td>
                                <td>
                                    <input class="date-picker form-control txt_GD" style="width: 170px;" type="date" placeholder="" value="@(readyForDispatched.GD.HasValue ? readyForDispatched.GD.Value.ToString("yyyy-MM-dd"): "")" />
                                </td>
                                <td>
                                    <input class="date-picker form-control txt_BL" style="width: 170px;" type="date" placeholder="" value="@(readyForDispatched.BL.HasValue ? readyForDispatched.BL.Value.ToString("yyyy-MM-dd"): "")" />
                                </td>
                                <td class="btn-group">
                                    <button type="button" class="btn btn-sm btn-block btn-success btn-v2 fs-8 text-nowrap mt-1 mb-0 btnSave">Save</button>&ensp;
                                    <button type="button" class="btn btn-sm btn-block btn-primary btn-v2 fs-8 text-nowrap mt-1 mb-0 btnReadyForDispatched">Ready For Dispatched</button>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <!-- LOADING SHOW HIDE -->
        <div class="loading-dimmer"><img src="~/AdminLte/dist/img/loading.gif" /></div>
    </div>
</div>
<script src="@Fingerprint.Tag("/AdminLte/Scripts/OrderBookings/ReadyForDispatched.js")"></script>
